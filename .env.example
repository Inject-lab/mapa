# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Mapbox Configuration
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token

# MapTiler Configuration (used for Satellite imagery)
VITE_MAPTILER_KEY=your_maptiler_key_here

# Satellite Provider (options: esri | maptiler | bing | mapbox)
VITE_SATELLITE_PROVIDER=esri

# Bing Maps Configuration (used if VITE_SATELLITE_PROVIDER=bing)
VITE_BING_MAPS_KEY=your_bing_maps_key_here

# Optional: Brasil API Configuration (uses default if not provided)
VITE_BRASIL_API_BASE_URL=https://brasilapi.com.br/api